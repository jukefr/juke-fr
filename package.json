{
  "name": "@kay/juke-fr",
  "version": "3.0.2",
  "license": "SEE LICENSE IN LICENSE",
  "author": "kay <git@juke.fr>",
  "bin": "./juke-fr",
  "files": [
    "juke-fr",
    ".next/standalone"
  ],
  "scripts": {
    "build": "NODE_ENV=production next build && cp -r ./.next/static ./.next/standalone/.next/ && cp -r public ./.next/standalone/",
    "export": "next build && next export && npm run fix-extensions",
    "fix-extensions": "find ./out -maxdepth 2 -name \"*.html.html\" -exec sh -c 'mv \"$1\" \"${1%.html.html}.html\"' _ {} \\;",
    "start": "next",
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "lint": "next lint",
    "test:setup": "npx playwright install",
    "test": "npm run build && playwright test",
    "tested-upgrade": "ncu -du --doctorInstall 'npm i' --doctorTest 'npm test'"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.19",
    "@chakra-ui/react": "2.7.1",
    "@chakra-ui/utils": "2.0.14",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@kay/console-feed": "^3.5.0",
    "@next/bundle-analyzer": "13.4.7",
    "@next/eslint-plugin-next": "13.4.7",
    "@playwright/test": "1.35.1",
    "@react-three/a11y": "3.0.0",
    "@react-three/fiber": "8.13.4",
    "@types/node": "20.3.2",
    "@types/prismjs": "1.26.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-lazy-load-image-component": "1.5.3",
    "@types/three": "0.152.1",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "babel-plugin-prismjs": "2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "framer-motion": "10.12.17",
    "next": "13.4",
    "npm-check-updates": "16.10.13",
    "prettier": "2.8.8",
    "prism-themes": "1.9.0",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-lazy-load-image-component": "1.6.0",
    "react-simple-code-editor": "0.13.1",
    "react-spring": "9.7.2",
    "sharp": "0.32.1",
    "three": "0.154.0",
    "typescript": "5.1.6"
  }
}
